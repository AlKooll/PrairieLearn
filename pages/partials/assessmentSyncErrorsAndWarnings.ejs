<% if (assessment.sync_errors) { %>
  <div class="alert alert-danger" role="alert">
    <h2 class="h5 alert-heading">Sync error</h2>
    <p>
      There was an error syncing this assessment; the information you see below
      may be inconsistent with this assessment's <code>infoAssessment.json</code> file.
      Please correct the error and sync again.
    </p>
    <pre class="bg-dark text-white rounded p-3 mb-0"><code><%= assessment.sync_errors %></code></pre>
  </div>
<% } %>
<% if (assessment.sync_warnings) { %>
  <div class="alert alert-warning" role="alert">
    <h2 class="h5 alert-heading">Sync warning</h2>
    <p>
      These warnings do not impact the ability to sync this assessment, but they
      should still be reviewed and corrected.
    </p>
    <pre class="bg-dark text-white rounded p-3 mb-0"><code><%= assessment.sync_warnings %></code></pre>
  </div>
<% } %>
