<% if (status == 'pending') { %>
    <span class="badge badge-secondary"> <i class="fa fa-sync-alt"></i> Pending </span>
<% } else if (status == 'approved') { %>
<span class="badge badge-success"> <i class="fa fa-check"></i> Approved </span>
<% } else if (status == 'denied') { %>
<span class="badge badge-anger"><i class="fa fa-times"></i> Denied </span>
<% } %>
